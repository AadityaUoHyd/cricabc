{
  "rewrites": [
    {
      "source": "/team/(?<id>[^/]+)",
      "destination": "/"
    },
    {
      "source": "/(.*)",
      "destination": "/"
    }
  ],
  "routes": [
    { "handle": "filesystem" },
    { "src": "/team/(?<id>[^/]+)", "dest": "/" },
    { "src": "/.*", "dest": "/" }
  ],
  "cleanUrls": true,
  "trailingSlash": false
}
